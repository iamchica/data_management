      "0    a      0\n",
      "1    b      1\n",
      "2    c      2\n",
      "3    d      3\n",
      "  key2  data2\n",
      "0    a      0\n",
      "1    b      1\n",
      "2    e      2\n",
      "  key1  data1 key2  data2\n",
      "0    a    0.0    a    0.0\n",
      "1    b    1.0    b    1.0\n",
      "2    c    2.0  NaN    NaN\n",
      "3    d    3.0  NaN    NaN\n",
      "4  NaN    NaN    e    2.0\n"
     ]
    }
   ],
   "source": [
    "mydata1 = pd.DataFrame({'key1': [\"a\",\"b\",\"c\",\"d\"], 'data1': range(4)})\n",
    "mydata2 = pd.DataFrame({'key2': [\"a\",\"b\",\"e\"], 'data2': range(3)})\n",
    "print(mydata1)\n",
    "print(mydata2)\n",
    "outer = pd.merge(mydata1, mydata2, left_on='key1', right_on='key2', how='outer')  \n",
    "print(outer)"
   ]
